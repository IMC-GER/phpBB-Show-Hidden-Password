{
    "name": "imcger/showhiddenpassword",
    "type": "phpbb-extension",
    "description": "Displays a button in the password field that allows you to make the hidden password visible.",
    "homepage": "",
    "version": "0.5.0",
    "time": "2023-11-03",
    "keywords": ["phpbb", "extension", "showhiddenpassword"],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Thorsten Ahlers",
            "homepage": "https://github.com/IMC-GER",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0.0",
        "phpbb/phpbb": "^3.3.0",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Show Hidden Passwort",
        "soft-require": {
            "phpbb/phpbb": "^3.3.0"
        },
        "version-check": {
            "host": "raw.githubusercontent.com",
            "directory": "/IMC-GER/phpBB-Show-Hidden-Password/master",
            "filename": "showhiddenpassword_version.json",
            "ssl": true
        }
    }
}
